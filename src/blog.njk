    <script>
               document.addEventListener("DOMContentLoaded", function () {
                 var toggleBtn = document.querySelector(".navbar-toggle");
                 var navbarCollapse = document.querySelector(".navbar-collapse");
               
                 // Toggle collapse on hamburger button click
                 toggleBtn.addEventListener("click", function () {
                   if (navbarCollapse.classList.contains("in")) {
                     // Hide nav
                     navbarCollapse.classList.remove("in");
                     navbarCollapse.style.height = "0px";
                   } else {
                     // Show nav
                     navbarCollapse.classList.add("in");
                     navbarCollapse.style.height = "auto";
                   }
                 });
               
                 // Close navbar on link click (for mobile)
                 var navLinks = document.querySelectorAll(".navbar-collapse a");
                 navLinks.forEach(function (link) {
                   link.addEventListener("click", function () {
                     if (navbarCollapse.classList.contains("in")) {
                       navbarCollapse.classList.remove("in");
                       navbarCollapse.style.height = "0px";
                     }
                   });
                 });
                 const carousel = document.getElementById('testimonialCarousel');
                        const carouselInner = carousel.querySelector('.carousel-inner');
                        const items = carouselInner.querySelectorAll('.item');
                        let activeIndex = 0;
                
                        function showItem(index) {
                            items.forEach((item, i) => {
                                if (i === index) {
                                    item.classList.add('active');
                                } else {
                                    item.classList.remove('active');
                                }
                            });
                        }
                
                        function nextItem() {
                            activeIndex = (activeIndex + 1) % items.length;
                            showItem(activeIndex);
                        }
                
                        function prevItem() {
                            activeIndex = (activeIndex - 1 + items.length) % items.length;
                            showItem(activeIndex);
                        }
                
                        // Add navigation buttons programmatically
                        const prevButton = document.createElement('button');
                        prevButton.innerHTML = '❮';
                        prevButton.style.position = 'absolute';
                        prevButton.style.top = '50%';
                        prevButton.style.transform = 'translateY(-50%)';
                        prevButton.style.left = '0';
                    //     prevButton.style.background = 'rgba(0, 0, 0, 0.5)';
                        prevButton.style.color = 'black';
                        prevButton.style.border = 'none';
                        prevButton.style.padding = '10px 15px';
                        prevButton.style.cursor = 'pointer';
                        prevButton.style.fontSize = '20px';
                        prevButton.addEventListener('click', prevItem);
                
                        const nextButton = document.createElement('button');
                        nextButton.innerHTML = '❯';
                        nextButton.style.position = 'absolute';
                        nextButton.style.top = '50%';
                        nextButton.style.transform = 'translateY(-50%)';
                        nextButton.style.right = '0';
                    //     nextButton.style.background = 'rgba(0, 0, 0, 0.5)';
                        nextButton.style.color = 'black';
                        nextButton.style.border = 'none';
                        nextButton.style.padding = '10px 15px';
                        nextButton.style.cursor = 'pointer';
                        nextButton.style.fontSize = '20px';
                        nextButton.addEventListener('click', nextItem);
                
                        carousel.appendChild(prevButton);
                        carousel.appendChild(nextButton);
                
                        // Initial display
                        showItem(activeIndex);
                
               });</script>
