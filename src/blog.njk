<div class="pagination-nav text-center mt-5" style="text-align: center;">
    <ul style="list-style: none; padding: 0; margin: 0; display: inline-flex;">
        
        {# --- 1. Previous Page Link --- #}
        {% if pagination.href.previous %}
            <li style="margin: 0 5px;">
                <a href="{{ pagination.href.previous | url }}" style="padding: 8px 15px; border: 1px solid #ddd; text-decoration: none;">&larr;</a>
            </li>
        {% endif %}

        {# --- 2. Numerical Pages Loop (The Working Array Index Fix) --- #}
        {% for href in pagination.hrefs %} 
            {% set pageNum = loop.index %} 
            
            <li style="margin: 0 5px;">
                <a 
                    {# Use the pagination.hrefs array directly for the URL #}
                    href="{{ href | url }}" 
                    style="
                        padding: 8px 15px; 
                        border: 1px solid #ddd; 
                        text-decoration: none; 
                        {% if pageNum == currentPage %}
                            background-color: #007bff; 
                            color: white;
                            border-color: #007bff;
                        {% else %}
                            background-color: white; 
                            color: black;
                        {% endif %}
                    "
                >{{ pageNum }}</a>
            </li>
        {% endfor %}

        {# --- 3. Next Page Link --- #}
        {% if pagination.href.next %}
            <li style="margin: 0 5px;">
                <a href="{{ pagination.href.next | url }}" style="padding: 8px 15px; border: 1px solid #ddd; text-decoration: none;">&rarr;</a>
            </li>
        {% endif %}

    </ul>
